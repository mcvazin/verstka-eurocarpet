.form-group label {
    position: relative;
    cursor: pointer;
    display: block;

}

.form-group label:before {
    content:'';
    background-color: transparent;
    border: 1px solid #a1a1a1;
    border-radius: 3px;

    display: inline-block;
    position: relative;
    vertical-align: middle;
    cursor: pointer;
    margin-right: 5px;
    width: 14px;
    height: 14px;
}

.form-group input:checked + label:after {
    content: '';
    display: block;
    position: absolute;
    top: 4px;
    left: 3px;
    border-radius: 50%;
    width: 8px;
    height: 8px;
    border: 0;
