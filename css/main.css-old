/** modal **/
.modal-body {
    padding: 0 35px 30px 35px;
}

.modal-content .close-popup {
    text-align: right;
    padding-right: 5px;
    padding-top: 3px;
}

.modal-content {
    border-radius: 0;
    border: 1px solid #6f1f71;
}

.modal-title {
    font-size: 1em;
    text-transform: uppercase;
    font-weight: 400;
    margin-bottom: 20px;
}

.modal-body input {
    max-width: 100%;
    height: 30px;
    padding: 0 10px;
    margin-bottom: 5px;
}

#call-back {
    width: 350px;
    margin: 0 auto;
}

.modal-button {
    background-image: -moz-linear-gradient(90deg, rgb(58, 151, 49) 0%, rgb(58, 151, 49) 5%, rgb(82, 194, 71) 7%, rgb(119, 213, 110) 100%);
    background-image: -webkit-linear-gradient(90deg, rgb(58, 151, 49) 0%, rgb(58, 151, 49) 5%, rgb(82, 194, 71) 7%, rgb(119, 213, 110) 100%);
    background-image: -ms-linear-gradient(90deg, rgb(58, 151, 49) 0%, rgb(58, 151, 49) 5%, rgb(82, 194, 71) 7%, rgb(119, 213, 110) 100%);
    text-shadow: 0px 0px 1.92px rgba(4, 4, 4, 0.75);

    border: none;
    color: #fff;
    text-align: center;
