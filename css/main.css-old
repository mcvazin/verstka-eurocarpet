.slick-slide img {
    display: block;
    margin: 0 auto;
}

.home-blog-slider .slick-slide img {
    width: 100%;
}

.home-slider-wrap {
    padding: 0;
    width: 100%;
}

.home-slider .home-slide {
    height: 100%;
    background-repeat: no-repeat !important;
    background-size: cover !important;
    background-position: center !important;
}

.home-slider .home-slider-inner {
    height: 420px;
    align-items: flex-end;
    padding-bottom: 65px;
    color: #fff;
}

.slick-arrow {
    position: absolute;
    top: calc(50% - 25px);
    z-index: 9;
    background: none;
    border: none;
    box-shadow: none;
    color: transparent;
    width: 0;
    height: 0;
    cursor: pointer;
}

.slick-prev.slick-arrow {
    left: calc(50% - 595px);
}

.slick-next.slick-arrow {
    right: calc(50% - 570px);
}

.slick-next.slick-arrow:before {
    content: '';
    background: url(../img/icons/all-slider-arrow-right.svg) no-repeat;
    width: 28px;
    height: 50px;
    display: block;
    transition: 0.3s all;
}

.slick-next.slick-arrow:hover:before {
    background: url(../img/icons/all-slider-arrow-right-g.svg) no-repeat;
    transition: 0.3s all;
}

.slick-prev.slick-arrow:before {
    content: '';
    background: url(../img/icons/all-slider-arrow-left.svg) no-repeat;
    width: 28px;
    height: 50px;
    display: block;
    transition: 0.3s all;
}

.slick-prev.slick-arrow:hover:before {
    background: url(../img/icons/all-slider-arrow-left-g.svg) no-repeat;
    transition: 0.3s all;
}

.home-slider .slick-next.slick-arrow:before {
    background: url(../img/icons/slider-arrow-right.svg) no-repeat;
}

.home-slider .slick-prev.slick-arrow:before {
    background: url(../img/icons/slider-arrow-left.svg) no-repeat;
}

.slider-more:hover {
    background: #1f7127;
    color: #fff;
    text-decoration: none;
}

.home-slider .slick-next.slick-arrow:hover:before {
    background: url(../img/icons/all-slider-arrow-right.svg) no-repeat;
    background-size: cover;
}

.home-slider .slick-prev.slick-arrow:hover:before {
    background: url(../img/icons/all-slider-arrow-left.svg) no-repeat;
    background-size: cover;
}


.home-slider .slick-prev.slick-arrow:before,
.home-slider .slick-next.slick-arrow:before{
	color:#fff;
}

